provider "aws" {
  region     = var.region
  version    = "~> 2.67.0"
  access_key = "{{access_key}}"
  secret_key = "{{secret_key}}"
}

terraform {
  backend "s3" {
    access_key = "{{access_key}}"
    secret_key = "{{secret_key}}"
    bucket     = "{{state_bucket}}"
    key        = "development-infrastructure/terraform.tfstate"
    region     = "{{region}}"
  }
}
